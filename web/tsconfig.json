{
    "compilerOptions": {
        "module": "commonjs",
        "strict": true,
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "useDefineForClassFields": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "allowJs": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "target": "es2019",
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {}
    }
}