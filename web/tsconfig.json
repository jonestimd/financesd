{
    "compilerOptions": {
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "lib": ["esnext", "dom"],
        "allowJs": true,
        "jsx": "preserve",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "target": "esnext",
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {}
    }
}